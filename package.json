{
  "name": "@cramkle/hipocampo",
  "version": "0.14.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/cramkle/hipocampo.git",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext ts,js .",
    "lint:ts": "tsc --noEmit --pretty",
    "start": "node --icu-data-dir=node_modules/full-icu dist/src/index.js",
    "dev": "tsc-watch --onSuccess \"npm start\"",
    "test": "jest",
    "release": "release-it",
    "download-schema": "apollo service:download --endpoint=http://localhost:5000/graphql -k",
    "gen-types": "apollo client:codegen __generated__ --target=typescript --localSchemaFile=schema.json --globalTypesFile=src/globalTypes.ts"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "dataloader": "^2.2.2",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.1.6",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.7",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-prom-bundle": "^6.6.0",
    "express-session": "^1.17.3",
    "full-icu": "^1.5.0",
    "graphql": "15.5.0",
    "graphql-relay": "^0.6.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.3.1",
    "i18next": "^19.8.7",
    "i18next-fs-backend": "^1.0.8",
    "i18next-http-middleware": "^3.3.0",
    "ioredis": "^4.27.9",
    "lodash": "^4.17.21",
    "mongoose": "^6.11.3",
    "morgan": "^1.9.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prom-client": "^13.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "shortid": "^2.2.13",
    "uuid": "^9.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@lucasecdb/eslint-config": "^0.15.0",
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/connect-redis": "^0.0.17",
    "@types/cookie-parser": "^1.4.1",
    "@types/draft-js": "^0.10.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/graphql-relay": "^0.6.0",
    "@types/graphql-type-json": "^0.3.0",
    "@types/helmet": "^4.0.0",
    "@types/i18next-fs-backend": "^1.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.10",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.13",
    "apollo": "^2.30.3",
    "babel-jest": "^27.1.0",
    "eslint": "^7.32.0",
    "husky": "^4.3.6",
    "jest": "^27.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "release-it": "^14.4.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.4.2"
  },
  "resolutions": {
    "graphql": "15.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ],
    "*.{scss,css}": [
      "stylelint"
    ]
  }
}
